body{font-size:13px;font-family: "Microsoft Yahei"; margin: 0 auto; }
.fixed-navbar-makeup {padding-top:10px}

a{color: #333; text-decoration: none}
a:hover{color: #ff9700; text-decoration: none;}
.clear{clear:both}
ul{ margin:0; }
li{ list-style-type:none; }

input.errorInput {border-color: none;outline: 0;outline: thin dotted 9;-webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px #f89406;-moz-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px #f89406;box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px #f89406;}
textarea, input[type="text"], input[type="password"], input[type="datetime"], input[type="datetime-local"], input[type="date"], input[type="month"], input[type="time"], input[type="week"], input[type="number"], input[type="email"], input[type="url"], input[type="search"], input[type="tel"], input[type="color"], .uneditable-input {
  box-shadow: none;
}
span.errorTip{padding-left:5px; color: red}
table td {vertical-align: top}
table.table-noborder {margin-bottom: 10px;}
table.table-noborder  td {border:none;}
table.table-noborder  th {border:none;}
.alert {border-radius: 0; text-shadow:none;}
small{ font-size: 100% }
.simpleeditor {margin-left: 0;}

.btn {border-radius: 2px;}

[class^="icon-"], [class*=" icon-"] {
  margin-top: 3px;
}

input.span1, textarea.span1, .uneditable-input.span1 {width: 72px;}
input.span6, textarea.span6, .uneditable-input.span6 {width: 425px;}
select, input[type="file"] {  height: 28px;line-height: 28px; }

.justcenter {text-align: center;}
.justright {text-align: right;}

.white-shadow {background: white; box-shadow: 0 1px 2px 0 rgba(0, 0, 0, 0.2); background:whiteSmoke;}

.front {position: relative; margin: 0px auto; }
.top {height: 73px; padding-top: 10px; box-shadow: 0px 1px 3px #ff9700;}

.front .top .btn {background:none; text-shadow: none; box-shadow: none;}
.front .top .btn-small {padding:0 10px; color:#3e3e3e; font-size:13px;}

.front .navbar-static-top { margin-left: 280px; margin-top: 62px;}
.front .navbar-inner { background: none; background-image: none; border: none;padding: 0; box-shadow: none; filter:none; }
.front .navbar-inner .container {width: 705px; margin: 0; }
.front .navbar .nav {margin: 0; float: right;}
.front .navbar .nav > li > a {
  font-size: 16px;
  text-shadow: 0 0px 0 rgba(0, 0, 0, 0);
  padding: 7.5px 20px 7.5px;
  /*cursor: default;*/
  background:none;
  color:#072;
}
/*.front .navbar .nav > li > a:hover { background: #072; color:white;}*/
/*.front .navbar .nav > li > a.active { background: #072;}*/

.front .dropdown-menu {min-width: 100px;}
.front .navbar .nav > li > .dropdown-menu:before {left:19px;}
.front .navbar .nav > li > .dropdown-menu:after {left:20px}

.front .dropdown-menu li > a { padding:10px; text-align: center; color: #072; font-size: 14px}
.front .dropdown-menu li > a:hover, .front .dropdown-menu li > a:focus, .front .dropdown-submenu:hover > a { 
  background-image:none; 
  background: none; 
  color: #F36F21;
  filter:none; 
}

.front .dropdown-submenu > a:after {border-left-color:#333; margin-right:5px;}
.front .dropdown-submenu:hover > a:after {border-left-color: #F36F21;}

ul.nav li.dropdown:hover > ul.dropdown-menu{ display: block; margin-top:0}
.navbar .nav li.dropdown.open > .dropdown-toggle, .navbar .nav li.dropdown.active > .dropdown-toggle, .navbar .nav li.dropdown.open.active > .dropdown-toggle {
  background: none;
  color: #F36F21;
  box-shadow: none;
  text-shadow: none;
}

.home-block-bg {width:1048px; text-align: center; position: relative; margin-top: -44px;}

.hx {font-size: 16px;  font-weight: lighter; line-height: 30px; font-family: simhei; width: 150px; border-bottom: 2px solid #F36F21; color: #F36F21; margin: 20px 0 10px 0;}

#footer {margin-top: 20px; background: #FFFAF3; color: #333; padding: 10px 0; border-top: 1px solid #ff9700;}
#footer a {color: #333}
#footer a:hover {color: #333; text-decoration: underline;}

.dialog{
  min-height: 200px;
  min-width: 300px;
  /*padding: 39px 19px 14px;*/
  background-color: white;
  border: 1px solid #DDD;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  border-radius: 4px;
}

.image_dialog_close{
  position: absolute;
  right: 0;
  font-size:22px;
  cursor:pointer;
  
}
.image_dialog_close:hover{
  text-decoration: none;
}

.fixed {
  position: fixed;
  top: 0;
}


.label-info[href], .badge-info[href] { background-color: #072; }
.label-important[href], .badge-important[href] {background-color: #fe9200;}

select, textarea, input[type="text"], input[type="password"], input[type="datetime"], input[type="datetime-local"], input[type="date"], input[type="month"], input[type="time"], input[type="week"], input[type="number"], input[type="email"], input[type="url"], input[type="search"], input[type="tel"], input[type="color"], .uneditable-input {
  border-radius: 0;
}
.again-recommend {
  margin-top: 20px;
}
.btn {
  text-shadow:none;
  background-image: none;
  border:1px solid #DDD;
  border-color:#DDD;
}

.btn-success {
  background: #ff9700;
  background-color: none;
  border-color: #ff9700; 
  text-shadow: none;
  border: none;
}
.btn-success:hover,
.btn-success:focus,
.btn-success:active,
.btn-success.active,
.btn-success.disabled,
.btn-success[disabled] {
  color: #ffffff;
  background-color: #EB8E07;
  *background-color: #EB8E07;
}

.register_block{background: #FCFCFC; padding: 40px; border: 1px solid #EEE;}
.register_block .control-group{margin-left: 180px;}
.register_block input[type=submit] {width: 100%}

.register_block h4{text-align: center;margin-bottom: 30px; font-weight: normal; font-size: 23px;}
.reg_image{margin: 20px 0;}
.register_left{float: left; width: 440px; }
.form-horizontal .control-label {width: 100px;}
.form-horizontal .controls {margin-left: 120px}
.register_right{float: right; width: 300px; }

.register_block .no_email_tips{margin-top: 25px;background: #f7f7f7;padding: 20px 30px 20px 25px;font-size: 12px;}
